<!DOCTYPE HTML>
<html>
<head>
  <title><%= @title %></title>
</head>
<body>
  <h1>Students (<%= Enum.count @students %>)</h1>
  <%= lc student_id inlist @students do %>
    <% {student_last, student_first, _student_bus} = InfoParse.Directory.get_student(student_id) %>
    <div>
      <a href="/student/<%= student_id%>" class="student_name"><%= student_last %>, <%= student_first %> </a>
    </div>
  <% end %>
</body>
</html>
